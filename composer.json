{
    "name": "dniccum/phone-number",
    "description": "A Laravel Nova phone number field with input masking and validation support.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "phone",
        "number"
    ],
    "authors": [
        {
            "name": "Doug Niccum",
            "email": "dniccumdesign@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.2",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "propaganistas/laravel-phone": "^5.0",
        "laravel/nova": "^4.0",
        "nova-kit/nova-packages-tool": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dniccum\\PhoneNumber\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dniccum\\PhoneNumber\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
